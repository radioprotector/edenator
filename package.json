{
  "name": "edenator",
  "version": "0.1.0",
  "description": "A browser-based music visualizer",
  "homepage": "https://radioprotector.github.io/edenator",
  "bugs": "https://github.com/radioprotector/edenator/issues",
  "author": "Cate Stock <radioprotector@users.noreply.github.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/radioprotector/edenator.git"
  },
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@react-spring/three": "^9.5.5",
    "@react-three/drei": "^9.32.4",
    "@react-three/fiber": "^8.8.7",
    "@react-three/postprocessing": "^2.6.2",
    "@types/node": "^18.7.23",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "env-cmd": "^10.1.0",
    "gh-pages": "^4.0.0",
    "jsmediatags": "3.9.3",
    "postprocessing": "^6.28.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "styled-components": "^5.3.6",
    "three": "^0.144.0",
    "typescript": "^4.8.4",
    "web-vitals": "^3.0.2",
    "zustand": "^4.1.1"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "npx gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "start:prod": "env-cmd -f .env.prodlocal react-scripts build && npx serve -s build -l 4000 -n",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">1%",
      "supports es6",
      "supports webgl2",
      "supports audio-api",
      "supports mp3 or supports ogg-vorbis or supports aac or supports flac",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jsmediatags": "3.9.3",
    "@types/styled-components": "^5.1.26",
    "@types/three": "^0.144.0"
  }
}
