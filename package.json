    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">1%",
      "supports webgl2",
      "supports audio-api",
      "supports mp3 or supports ogg-vorbis or supports aac or supports flac",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jsmediatags": "^3.9.3",
    "@types/styled-components": "^5.1.19",
    "@types/three": "^0.135.0"
